<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrackApp">
        <activity android:name=".Timetable"></activity>
        <activity android:name=".studentinformation" />
        <activity android:name=".Addtask" />
        <activity android:name=".Addnotes" />
        <activity android:name=".Groupassignments" />
        <activity android:name=".Mydata" />
        <activity android:name=".Homepage" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>